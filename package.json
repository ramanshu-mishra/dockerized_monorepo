  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.6.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@types/cors": "^2.8.19",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "prisma": "^7.1.0",
    "tailwind-merge": "^3.4.0",
    "ws": "^8.18.3"
